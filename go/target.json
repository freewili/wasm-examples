{
    "inherits": [ "wasm-unknown" ],
    "opt": "z",
    "size": "short",
    "print-allocs": true,
    "gc": "leaking",
    "scheduler": "none",
    "no-debug": true,
    "default-stack-size": 49152,
    "ldflags": [
        "--initial-memory=65536",
        "--max-memory=65536",
        "-zstack-size=49152"
    ],
    "flash-command": "fwi-serial -m 1 -fn /scripts/go.wasm -w go -s {hex}"
}
